import java.util.*;

class Number {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a,b;
        System.out.println("\nEnter two integer numbers :");
        a=sc.nextInt();
        b=sc.nextInt();
        
        System.out.println("Sum of two ineteger numbers is: " + (a+b));
    }
}
