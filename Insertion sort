public class Main {
  public static void main(String[] args) {
    //System.out.println("Hello world!");
    int a[]={2,8,4,1};
    // int ans =insertion(a);
    System.out.println(insertion(a));
  }
  public static void insertion(int a[]){
    for(int i=0;i<a.length-1;i++){
      for(int j=i+1;j>0;j--){
        if(a[j]<a[j-1]){
        //  swap(a,j,j-1);
          int temp= a[j];
          a[j]=a[j-1];
          a[j-1]=temp;
            
        }
      }
    }
    
  }
  
}
